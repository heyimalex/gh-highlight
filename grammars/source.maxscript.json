        {
          "name": "constant.numeric.time.maxscript",
          "match": "(?\u003c!\\w|\\.)(?:\\d*\\.\\d+|\\d+\\.?\\d*)[msft](?:(?:\\d+|\\d*\\.\\d*)[msft])*"
