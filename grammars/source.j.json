{
  "fileTypes": [
    "ijs"
  ],
  "name": "J",
  "patterns": [
    {
      "name": "string.j",
      "match": "('[^']*')"
    },
    {
      "name": "constant.numeric.j",
      "match": "(_?\\d*\\.?\\d+)"
    },
    {
      "name": "comment.j",
      "match": "(?:^|[ )])(NB\\..*$)"
    },
    {
      "name": "copula.global.j",
      "match": "=\\:"
    },
    {
      "name": "copula.local.j",
      "match": "=\\."
    },
    {
      "name": "keyword.j",
      "match": "(?:^|(?<=[) ]))((?:while\\.)|(?:whilst\\.)|(?:if\\.)|(?:elseif\\.)|(?:else\\.)|(?:do\\.)|(?:end\\.))"
    },
    {
      "name": "noun.j",
      "match": "(?:_(?=[^\\d:]))|(?:a[.:])"
    },
    {
      "name": "conjunction.j",
      "match": "(?:\\^\\:)|(?:[.:`@][.:]?)|(?:;\\.)|(?:![.:])|(?:[\"](?![.:]))|(?:\\&\\.?\\:?)|(?:[dHT]\\.)|(?:D[.:])|(?:[LS]\\:)"
    },
    {
      "name": "adverb.j",
      "match": "(?:[~}](?![.:]))|(?:[/\\\\]\\.?)|(?:[bfM]\\.)|(?:t[.:])"
    },
    {
      "name": "verb.j",
      "match": "(?:[=!](?![.:]))|(?:[<>+*%$|,#{-][.:]?)|(?:_\\:)|(?:\\^\\.?)|(?:\\^\\!\\.)|(?:[~}\"ip][.:])|(?:[;\\[]\\:?)|(?:[/\\\\]\\:)|\\]|(?:\\{\\:\\:)|(?:\\?\\.?)|(?:[AeEIjLr]\\.)|(?:p\\.\\.)|(?:[qsux0]\\:)|(?:_?[1-9]\\s?\\:)"
    }
  ],
  "scopeName": "source.j"
}