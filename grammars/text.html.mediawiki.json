          "applyEndPatternLast": 1,
          "begin": "(?x:\n\t\t\t\t\t\t(\\[\\[)                         # opening brackets\n\t\t\t\t\t\t  ( [ ]*(((i|I)mage)(:))       # spaces, image, colon\n\t\t\t\t\t\t    ([^\\[\\]|]+)                # anything\n\t\t\t\t\t\t    (?<!\\s)[ ]*                # spaces\n\t\t\t\t\t\t  )\n\t\t\t\t\t)",
