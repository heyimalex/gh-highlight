{
  "name": "SourcePawn",
  "scopeName": "source.sp",
  "patterns": [
    {
      "name": "comment.sourcepawn",
      "match": "\\/\\/.*"
    },
    {
      "name": "comment.block.sourcepawn",
      "begin": "/\\*",
      "end": "\\*/",
      "captures": {
        "0": {
          "name": "comment.sourcepawn"
        }
      }
    },
    {
      "name": "string.sourcepawn",
      "match": "\"(\\\\\"|\\\\.|[^\\\\\"])*\""
    },
    {
      "name": "string.sourcepawn",
      "match": "'(\\\\.|[^\\\\])?'"
    },
    {
      "name": "keyword.operator.sourcepawn",
      "match": "(\\#include|\\#tryinclude)\\s+(\u003c|\")(.+)(\u003e|\")",
      "captures": {
        "1": {
          "name": "keyword.control.import.sourcepawn"
        },
        "3": {
          "name": "storage.type.sourcepawn"
        }
      }
    },
    {
      "match": "(\\#(define|undef|pragma))\\s+(\\w+)",
      "captures": {
        "1": {
          "name": "keyword.control.import.sourcepawn"
        },
        "3": {
          "name": "constant.sourcepawn"
        }
      }
    },
    {
      "name": "keyword.operator.sourcepawn",
      "match": "(\\#if)\\s+!*(\\w+)\\s+(\\w+)",
      "captures": {
        "1": {
          "name": "keyword.control.import.sourcepawn"
        },
        "2": {
          "name": "declaration.tag.sourcepawn"
        },
        "3": {
          "name": "constant.sourcepawn"
        }
      }
    },
    {
      "name": "keyword.control.import.sourcepawn",
      "match": "#(else|end(if|input))"
    },
    {
      "match": "(enum|struct)(\\s+(\\w+))*",
      "captures": {
        "1": {
          "name": "declaration.tag.sourcepawn"
        },
        "3": {
          "name": "storage.type.sourcepawn"
        }
      }
    },
    {
      "name": "storage.type.sourcepawn",
      "match": "\\w+(?=:)"
    },
    {
      "name": "keyword.operator.sourcepawn",
      "match": "{|}|-|=|!|%|\u0026|\\(|\\)|,|\\.|:|;|\\?|@|\\[|\\]|\\^|\\||~|\\+|\u003c|\u003e"
    },
    {
      "name": "constant.numeric.sourcepawn",
      "match": "\\b((0(x|X)[0-9a-fA-F]*)|(([0-9]+\\.?[0-9]*)|(\\.[0-9]+))((e|E)(\\+|-)?[0-9]+)?)(L|l|UL|ul|u|U|F|f|ll|LL|ull|ULL)?\\b"
    },
    {
      "name": "declaration.tag.sourcepawn",
      "match": "(\\b)(for|if|else|do|while|switch|case|default|return|break|continue|new|decl|public|stock|const|forward|static|funcenum|functag|native)(\\b)"
    },
    {
